@model  Flights.Models.FlightViewModel
<script src="~/Scripts/datepicker.js"></script>
@using (Html.BeginForm("IndexPost", "Test", FormMethod.Post, new { @id = "frmtest" }))
{

    <div class='editor-label'>
        @Html.LabelFor(model => model.Date1)
    </div>
    <div class='editor-field'>
        @Html.TextBoxFor(model => model.Date1, new { @class = "datetype", type = "text" })
        @Html.ValidationMessageFor(model => model.Date1)
    </div>
    <button type="submit">submit</button>

    <div class='editor-label'>
        @Html.LabelFor(model => model.Date2)
    </div>
    <div class='editor-field'>
        @Html.TextBoxFor(model => model.Date2, new { @class = "datetype", type = "text" })
        @Html.ValidationMessageFor(model => model.Date2)
    </div>
    <button type="submit">submit</button>

}